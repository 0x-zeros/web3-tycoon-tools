{
  "name": "web3-tycoon-asset-generator",
  "version": "1.0.0",
  "description": "AIGC美术资源批量生成工具 - Web3 Tycoon游戏专用",
  "main": "asset_generator.js",
  "scripts": {
    "generate": "node asset_generator.js",
    "generate:core": "node asset_generator.js core 20",
    "generate:sample": "node asset_generator.js sample",
    "generate:single-sample": "node asset_generator.js single-sample",
    "generate:tiles": "node asset_generator.js category tiles",
    "generate:ui": "node asset_generator.js category ui",
    "generate:icons": "node asset_generator.js category icons",
    "generate:cards": "node asset_generator.js category cards",
    "generate:characters": "node asset_generator.js category characters",
    "generate:dice": "node asset_generator.js category dice",
    "generate:gpt": "node asset_generator.js --model gpt-image-1 --size 1024x1024",
    "generate:gpt:tiles": "node asset_generator.js category tiles --model gpt-image-1 --size 1024x1024",
    "generate:fast": "node asset_generator.js core 10",
    "test": "node -e \"console.log('Asset generator ready!')\"",
    "clean": "rm -rf output/* logs/*",
    "setup": "mkdir -p output logs && cp .env.example .env"
  },
  "keywords": [
    "aigc",
    "game-assets",
    "dall-e-3",
    "web3",
    "monopoly",
    "cocos-creator",
    "ai-generated",
    "batch-generation"
  ],
  "author": "Web3 Tycoon Team",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "openai": "^5.16.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
